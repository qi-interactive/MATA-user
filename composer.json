{
    "name": "mata/mata-user",
    "description": "Flexible user registration and authentication module for MATA",
    "keywords": ["yii2", "mata-user", "user management", "mata"],
    "type": "mata-module",
    "license": "MIT",
    "support": {
    },
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-swiftmailer": "~2.0",
        "yiisoft/yii2-authclient": "~2.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "autoload": {
        "psr-4": {
            "mata\\user\\": ""
        }
    },
    "extra": {
        "bootstrap": "mata\\user\\Bootstrap",
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}